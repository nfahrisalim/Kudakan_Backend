modules = ["python-3.11"]

[nix]
channel = "stable-24_05"
packages = ["libxcrypt"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "FastAPI Server"

[[workflows.workflow]]
name = "FastAPI Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "pip install fastapi uvicorn sqlalchemy psycopg2-binary pydantic[email] minio python-multipart && python main.py"
waitForPort = 8000

[deployment]
run = ["sh", "-c", "pip install fastapi uvicorn sqlalchemy psycopg2-binary pydantic[email] minio python-multipart && python main.py"]
